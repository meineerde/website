---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>meine erde</title>
 <link href="http://meine-er.de/atom.xml" rel="self"/>
 <link href="http://meine-er.de/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://meine-er.de/</id>
 <author>
   <name>Holger Just</name>
   <email>web@meine-er.de</email>
 </author>

 {% for post in site.posts limit: 15 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://meine-er.de{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://meine-er.de{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>